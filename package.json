{
  "name": "full-stack-node-react",
  "version": "1.0.0",
  "description": "full stack react and node app",
  "main": "index.js",
  "author": "gyang",
  "license": "MIT",
  "scripts": {
    "server": "babel-node ./server/app.js",
    "client": "webpack-dev-server --mode development --open --hot",
    "start": "concurrently \"yarn run server\" \"yarn run client\"",
    "test": "mocha $(find server client -name '*spec.js') --require babel-core/register --require babel-polyfill  --require ./client/setupTest.js",
    "test-watch": "nodemon --exec 'yarn test'",
    "coverage": "nyc --reporter=html mocha $(find server client -name '*.spec.js') --require babel-core/register --require babel-polyfill --require ./client/setupTest.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "^12.1.0",
    "mocha": "^5.2.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.3.4",
    "style-loader": "^0.23.0",
    "supertest": "^3.3.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}

{
  "presets": ["env", "react"],
  "plugins": [
    "transform-object-rest-spread",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "development": {
      "sourceMaps": false,
      "retainLines": true
    }
  }
}
